#set ($irccontext = $services.ircbot.context)
#set ($date = $datetool.get('yyyyMMdd'))
#if ("$!irccontext" != "")
  #set ($channel = "$!irccontext.channel.name")
  #if ("$!channel" == "")
    #set ($channelNameIterator = $!irccontext.bot.channelsNames.iterator())
    #if ($channelNameIterator.hasNext())
      #set ($channel = "$channelNameIterator.next()")
    #else
      #set ($channel = '')
    #end
  #end
  #if ("$!channel" != '')
    #set ($channel = $stringtool.stripStart($channel, '#'))
    IRC.${channel}Archive${date}
  #end
#end
